{
  "name": "ivoryui-ivcon",
  "description": "Pure css icons by IVORY UI",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IVORY-UI/ivcon.git"
  },
  "keywords": [
    "css",
    "icons"
  ],
  "scripts": {
    "build:styles": "sass --style=compressed src/scss/ivcon.scss dist/ivcon.min.css",
    "copy:files": "cp package.json dist/ && cp README.md dist/",
    "build": "npm run build:styles && npm run copy:files",
    "prepare:demo": "cp dist/ivcon.min.css demo/"
  },
  "devDependencies": {
    "sass": "^1.77.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": {
    "name": "Lakshmikanth Vallampati",
    "url": "https://github.com/kanthvallampati/"
  },
  "license": "MIT"
}
