{
  "name": "ivoryui-ivcon",
  "description": "Pure css icons by IVORY UI",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IVORY-UI/ivcon.git"
  },
  "keywords": [
    "css",
    "icons"
  ],
  "files": [
     "dist/",
     "README.md"
   ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:styles": "sass --style=compressed --source-map src/scss/ivcon.scss dist/ivcon.min.css",
    "build": "npm run clean && npm run build:styles",
    "watch:styles": "sass --watch src/scss/ivcon.scss:dist/ivcon.min.css --style=compressed --source-map"
  },
  "devDependencies": {
    "sass": "^1.77.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": {
    "name": "Lakshmikanth Vallampati",
    "url": "https://github.com/kanthvallampati/"
  },
  "license": "MIT"
}
